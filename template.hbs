<html>
  <head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <title>RSS by email</title>
  </head>
  <body>
    <ul>
      {{#each items}}
        <li>
          <h3><a href="{{url}}">{{title}}</a></h3>
          <ul>
            {{#each entries}}
              <li>
                <a href="{{url}}">{{title}}</a>
              </li>
            {{/each}}
          </ul>
        </li>
      {{/each}}
    </ul>
  </body>
</html>